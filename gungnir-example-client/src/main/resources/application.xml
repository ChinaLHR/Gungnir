<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="IDataServicce" class="io.github.chinalhr.gungnir.netchannel.client.GungnirClientProxy">
        <property name="iclass" value="io.github.chinalhr.api.IDataService" />
        <property name="serializer" value="protostuff"/>
        <property name="timeoutMillis" value="5000"/>
        <property name="version" value="1.0.0"/>
        <property name="groupName" value="dev"/>
        <property name="loadBalance" value="poll"/>
    </bean>
</beans>